# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Google Gemini API Key (for AI metadata generation)
# Get your key at: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your-gemini-api-key-here

# Backend URL (for local development)
# Use http://localhost:3000 for local backend
# Use your deployed backend URL for production
VITE_BACKEND_URL=http://localhost:3000

# Backend WebSocket URL (for streaming)
# Use ws://localhost:3000 for local development
# Use wss://your-backend-url for production (secure WebSocket)
VITE_BACKEND_WS_URL=ws://localhost:3000
